
## targets

default: isa-launchbury
images: 
test: isa-launchbury

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i true -g true -d pdf -D generated


## isa-launchbury

isa-launchbury: $(LOG)/HOLCF-Nominal2-isa-launchbury.gz

$(LOG)/HOLCF-Nominal2-isa-launchbury.gz: isa-launchbury/ROOT.ML isa-launchbury/document/root.tex isa-launchbury/*.thy
	@$(USEDIR) HOLCF-Nominal2 isa-launchbury


## clean

clean:
	@rm -f $(LOG)/HOLCF-Nominal2-isa-launchbury.gz
